{
    "compilerOptions": {
        "outDir": "./dist/",
        "module": "commonjs",
        "target": "es5",
        "lib": ["es6", "dom"],
        "sourceMap": true,
        "allowJs": true,
        "jsx": "react",
        "moduleResolution": "node",
        "noImplicitThis": true,
        "strictNullChecks": false,
        "esModuleInterop": true
    },
    "include": [
        "./src/**/*", "server.js"
    ]
}