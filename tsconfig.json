{
    "compilerOptions": {
        "outDir": "./dist/",
        "target": "es5",
        "lib": ["es5", "es6", "dom"],
        "jsx": "react",
        "allowSyntheticDefaultImports": true,
        "module": "ESNext",
        "moduleResolution": "node",
        "forceConsistentCasingInFileNames": true,
        "listEmittedFiles": true,
        "noUnusedLocals": true
    },
    "include": [
        "./src/**/*", "server.js"
    ]
}